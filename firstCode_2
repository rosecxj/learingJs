var cxjObject={
    phone:'15827336288',
    gender:'female',
    age:18,
    coffeeList:['Americano','Latte','Mocha','Cappuccino'],
    isMemOfCCP:true
}

switch (cxjObject.age){
    case 18:
        console.log('神奇的Rose永远18');
    break;
    case 19:
        console.log('神奇的Rose明年19岁');
    break;
    default:
        console.log('神奇的Rose');
     break;           
}
